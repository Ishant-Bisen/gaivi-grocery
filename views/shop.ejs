                "product": button.getAttribute('data-product')
            }
            $.ajax({
                type: 'POST',
                url: '/cart',
                data: s,
                dataType: "json",
                success: function (response) {

                    setTimeout(function () {
                        document.getElementById("check").innerHTML = "";
